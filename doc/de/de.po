#, no-c-format
msgid "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;mv \"FluidR3 GM.SF2\" FluidR3.sf2"
msgstr "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;mv \"FluidR3 GM.SF2\" FluidR3.sf2"

