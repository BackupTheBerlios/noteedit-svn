msgid "staff %1; measure  %2 : %3 128th, should be: %4\n"
msgstr "Zeile %1; Takt %2 %3: 128tel; es sollten %4 sein\n"

#: filehandler.cpp:610
msgid ""
"staff %1; measure %2: Mup cannot deal with mixed lyrics/non-lyrics notes in "
"the same measure\n"
msgstr ""
"Zeile %1; Takt %2: Mup kann nicht mit gemischt: Noten mit und ohne Text im "
"selben Takt umgehen\n"

#: filehandler.cpp:615
msgid ""
"staff %1; measure %2: Mup up cannot deal with slurs over more than 1 "
"element, besides in 1st or 2nd voice\n"
msgstr ""
"Zeile %1: Takt %2: Mup kann nicht mit Bindebögen umgehen, die mehr alsein "
"Element überstreichen, es sei denn es ist die erste oder 2.Stimme.\n"

#: filehandler.cpp:620
msgid "staff %1; measure %2: Mup cannot deal with trills without \"tr\" sign\n"
msgstr ""
"Zeile %1; Takt %2: MUP kann nicht mit Trillern ohne \"tr\"-Symbol umgehen.\n"

#: filehandler.cpp:625
msgid "Staff %1 has irregular key, this may not work with MUP.\n"
msgstr ""
"Zeile %1: hat eine ungebräuchliche Vorzeichnung! Damit kann MUP nicht "
"umgehen.\n"

#: filehandler.cpp:630
msgid "Staff %1 has too many voices! MUP accepts up to 3 voices.\n"
msgstr "Zeile %1: hat zu viele Stimmen! MUP akzeptiert nur bis zu 3 Stimmen!\n"

#: filehandler.cpp:635
msgid "Staff %1 contains drum staff. This cannot be expressed in MUP.\n"
msgstr ""
"Zeile %1 enthält Schlagzeugelemente. Das kann in MUP nicht ausgedrückt "
"werden.\n"

#: filehandler.cpp:640
msgid "staff %1; measure %2: Mup cannot deal with costum beams in graces\n"
msgstr ""
"Zeile %1; Takt %2: MUP kann nicht mit vorgegebenen Balken im Vorschlag "
"umgehen.\n"

#: filehandler.cpp:645
msgid "staff %1; measure %2: Mup cannot deal with graces after\n"
msgstr "Zeile %1; Takt %2: MUP kennt keinen Nachschlag.\n"

#: filehandler.cpp:650
msgid ""
"staff %1; measure %2: MUP cannot deal with clef changes in the middle of "
"measure\n"
msgstr ""
"Zeile %1; Takt %2: Mup kann nicht mit Notenschlüsselwechseln in der "
"Taktmitte umgehen.\n"

#: filehandler.cpp:655
msgid ""
"staff %1; measure %2: MUP cannot deal with key changes in the middle of "
"measure\n"
msgstr ""
"Zeile %1; Takt %2: Mup kann nicht Vozeichenwechseln in der Taktmitte "
"umgehen.\n"

#: filehandler.cpp:660
msgid ""
"MUP cannot deal with some chord annotations with and some without grids\n"
